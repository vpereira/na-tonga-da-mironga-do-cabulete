<p id="notice"><%= notice %></p>

<h1>Package Files</h1>

<table>
  <thead>
    <tr>
      <th>Path</th>
      <th>Content</th>
      <th>Package</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @package_files.each do |package_file| %>
      <tr>
        <td><%= package_file.path %></td>
        <td><%= package_file.content %></td>
        <td><%= package_file.package %></td>
        <td><%= link_to 'Show', package_file %></td>
        <td><%= link_to 'Edit', edit_package_file_path(package_file) %></td>
        <td><%= link_to 'Destroy', package_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package File', new_package_file_path %>
